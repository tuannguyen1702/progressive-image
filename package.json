{
  "name": "ny-image",
  "version": "0.0.2",
  "description": "Progressive Image is a web component, this component has developed base on stenciljs, build to support all js framework & pure js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ny-image/ny-image.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "author": "Tuan Nguyen",
  "keywords": [
    "Progressive Image",
    "Image Loaeder",
    "Lazy-load Image",
    "Web Component",
    "Stenciljs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tuannguyen1702/progressive-image.git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
